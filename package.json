{
  "name": "property-management",
  "version": "1.0.0",
  "description": "Property Management System Backend",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "prebuild": "rimraf dist && npm install rimraf -g",
    "build": "tsc && npm run copy-migrations",
    "copy-migrations": "node scripts/copy-migrations.js",
    "lint": "eslint . --ext .ts",
    "seed:admin": "ts-node src/scripts/seedAdmin.ts",
    "typeorm": "typeorm-ts-node-commonjs -d src/data-source.ts",
    "migration:generate": "npx typeorm-ts-node-commonjs -d src/data-source.ts migration:generate",
    "migration:run": "npx typeorm-ts-node-commonjs -d src/data-source.ts migration:run",
    "migration:revert": "npx typeorm-ts-node-commonjs -d src/data-source.ts migration:revert"
  },
  "keywords": [
    "property",
    "management",
    "typescript",
    "node",
    "express",
    "mysql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/validator": "^13.12.2",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.1",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.21",
    "typeorm-extension": "^3.7.0",
    "typescript": "^5.3.3",
    "validator": "^13.12.0",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/pg": "^8.11.11",
    "nodemon": "^3.1.0"
  }
}
