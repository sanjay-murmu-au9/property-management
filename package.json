  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.10",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.13.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.21",
    "typescript": "^5.8.2"
  }
